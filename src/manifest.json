{
  "manifest_version": 3,

  "name": "Solid Identity Manager",
  "description": "Manage and switch between your Solid identities",
  "version": "1.0",

  "icons": {
    "16": "id-badge-16.png",
    "32": "id-badge-32.png",
    "48": "id-badge-48.png",
    "128": "id-badge-128.png"
  },

  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],

  "web_accessible_resources": [
    {
      "resources": [ "icons/*" ],
      "matches": ["<all_urls>"]
    }
  ],

  "host_permissions": [
    "<all_urls>"
  ],

  "action": {
    "default_popup": "popup.html",
    "default_title": "Solid Authentication"
  },

  "background": {
    "service_worker": "sw.js"
  },

  "externally_connectable": {
    "matches": ["<all_urls>"]
  },
  "key": ""
}
